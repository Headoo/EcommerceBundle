  "homepage" : "https://github.com/Headoo/HeadooEcommerceBundle",
  "type" : "symfony-bundle",
  "authors" : [{
    "name" : "Ana Luiza Cicconi",
    "email" : "analuizacicconi@gmail.com"
  }],
  "keywords" : [
    "ecommerce bundle"
  ],
  "license" : [
    "MIT"
  ],
  "require" : {
    "php": ">=5.3.3",
